{
  "name": "batch-image-converter",
  "description": "Convert images in batch in the browser.",
  "private": false,
  "version": "1.3.1",
  "type": "module",
  "main": "./dist/image-converter.umd.cjs",
  "module": "./dist/image-converter.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/image-converter.js",
      "require": "./dist/image-converter.umd.cjs"
    }
  },
  "files": [
    "dist/*.css",
    "dist/*.js",
    "dist/*.cjs",
    "dist/*.d.ts"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": "https://github.com/tony-xlh/Image-Converter",
  "author": "xulihang <xulihanghai@gmail.com> (https://github.com/xulihang)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tony-xlh/Image-Converter/issues"
  },
  "homepage": "https://github.com/tony-xlh/Image-Converter#readme",
  "keywords": [
    "tiff",
    "pdf",
    "jpg",
    "bmp",
    "convert",
    "dynamsoft"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@types/node": "^20.4.4",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.3.1"
  },
  "dependencies": {
    "dwt": "^18.0.2",
    "jszip": "^3.10.1"
  }
}
